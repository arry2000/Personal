
@{
    ViewBag.Title = "Brusland Mountains";
    ViewBag.Active = "WorldMap";
}

<div class="row">
    <div class="col-12">
        <h2>Brusland Mountains @Html.ActionLink("Back to map", "WorldMap", "DandD", null, new { @class = "btn btn-primary pull-right" })</h2>
        <hr />
    </div>
</div>

<div class="row mb-3">
    <div class="col-4">
        <img src="~/Content/images/maps/Brusland_Mountains_Cities.jpg" class="img-fluid" alt="Brusland Mountains cities" />
    </div>

    <div class="col-4">
        <img src="~/Content/images/maps/Brusland_Mountains_Landscape.jpg" class="img-fluid" alt="Brusland Mountains landscape" />
    </div>

    <div class="col-4">
        <img src="~/Content/images/maps/Brusland_Mountains_Nobles.jpg" class="img-fluid" alt="Brusland Mountains nobles" />
    </div>

</div>
