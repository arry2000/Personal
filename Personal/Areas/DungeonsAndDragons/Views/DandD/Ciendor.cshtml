
@{
    ViewBag.Title = "Ciendor";
    ViewBag.Active = "WorldMap";
}

<div class="row">
    <div class="col-12">
        <h2>Ciendor @Html.ActionLink("Back to map", "WorldMap", "DandD", null, new { @class = "btn btn-primary pull-right" })</h2>
        <hr />
    </div>
</div>

<div class="row mb-3">
    <div class="col-4">
        <img src="~/Content/images/maps/Ciendor_Cities.jpg" class="img-fluid" alt="Ciendor cities" />
    </div>

    <div class="col-4">
        <img src="~/Content/images/maps/Ciendor_Landscape.jpg" class="img-fluid" alt="Ciendor landscape" />
    </div>

    <div class="col-4">
        <img src="~/Content/images/maps/Ciendor_Nobles.jpg" class="img-fluid" alt="Ciendor nobles" />
    </div>

</div>
